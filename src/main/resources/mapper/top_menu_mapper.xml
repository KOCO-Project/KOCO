<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
					    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="topmenu">
	<select id="getlist" resultType='info'>
		<![CDATA[
			SELECT INFO_NO as infoNo, INFO_NAME as infoName
			FROM KOCO_BOARD_INFO
			ORDER BY INFO_NO
		]]>
	</select>
</mapper>